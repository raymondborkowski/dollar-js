{
    "extends": "eslint:recommended",

    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "$": false,
        "utils": true,
        "arrPush": false,
        "arrSlice": false,
        "docConstruct": false,
        "docElement": false,
        "elemProto": false,
        "fnType": false,
        "getElementData": false,
        "getInternalElementId": false,
        "getNodes": false,
        "matchesSelector": false,
        "normalizeContext": false,
        "objHasProp": false,
        "objToString": false,
        "parseHTML": false,
        "pushElementData": false,
        "setElementData": false,
        "strType": false,
        "undef": false
    },

    "rules": {

        "curly": 2,
        "dot-notation": 1,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-cond-assign": 0,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-debugger": 1,
        "no-empty": 1,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-semi": 1,
        "no-loop-func": 2,
        "no-new-func": 2,
        "no-sequences": 2,
        "no-shadow-restricted-names": 2,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "quote-props": [1, "consistent-as-needed"],
        "semi": 2,
        "wrap-iife": [2, "any"],
        "yoda": 1,
        "no-inner-declarations": 1,

        "no-multi-spaces": 1,
        "brace-style": 1,
        "comma-spacing": 1,
        "comma-style": 1,
        "eol-last": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 1,
        "quotes": [1, "single", "avoid-escape"],
        "space-before-blocks": 1,
        "space-before-function-paren": 1,
        "space-infix-ops": 1,

    }
}
